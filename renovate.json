{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>descope-sample-apps/renovate-config"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^.*$/"
      ],
      "matchStrings": [
        "unpkg.com/(?<depName>.*)@(?<currentValue>[^/]*)"
      ],
      "datasourceTemplate": "npm"
    }
  ],
  "packageRules": [
    {
      "followTag": "latest",
      "minimumReleaseAge": null,
      "matchPackageNames": [
        "@descope/{/,}**"
      ]
    }
  ]
}
